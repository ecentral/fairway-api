{
    "name": "fairway/filesystem-api",
    "type": "library",
    "description": "Library providing an common interface for various fairway APIs.",
    "homepage": "https://github.com/ecentral/fairway-api/",
    "license": "MIT",
    "keywords": [
        "Pixelboxx",
        "API"
    ],
    "authors": [
        {
            "name": "Christian Rodriguez Benthake",
            "role": "Developer",
            "email": "c.rodriguez.benthake@ecentral.de"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.4",
        "fairway/canto-saas-api": "*",
        "fairway/pixelboxx-saas-api": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.8",
        "friendsofphp/php-cs-fixer": "^v3.12",
        "phpro/grumphp": "^1.10",
        "pestphp/pest": "^1.22"
    },
    "autoload": {
        "psr-4": {
            "Fairway\\FairwayFilesystemApi\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp": true,
            "pestphp/pest-plugin": true
        }
    }
}
